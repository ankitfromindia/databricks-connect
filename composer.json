{
    "name": "ankitfromindia/databricks",
    "description": "Create ODBC Connection to Starbust and run query",
    "keywords": [
        "ankitfromindia",
        "databricks"
    ],
    "homepage": "https://github.com/ankitfromindia/databricks",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ankit Vishwakarma",
            "email": "er.ankitvishwakarma@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Ankitfromindia\\Databricks\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ankitfromindia\\Databricks\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ankitfromindia\\Databricks\\DatabricksServiceProvider"
            ],
            "aliases": {
                "Databricks": "Ankitfromindia\\Databricks\\DatabricksFacade"
            }
        }
    }
}